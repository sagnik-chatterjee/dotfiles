[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

[color]
	diff=auto 
	status=auto 
	branch=auto 
	interactive=auto 
	ui =true 
	pager=true 

[color "status"]
	added=green 
	changed=red bold 
	untracked=magenta bold 

[color "branch"]
	remote=yellow 
